{
  "name": "ember-material-lite",
  "version": "0.0.0-development",
  "description": "Material Design Lite for Ember.js Apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember try:each",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "repository": "https://github.com/mike-north/ember-material-lite.git",
  "homepage": "https://github.com/mike-north/ember-material-lite",
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@ember/optional-features": "0.7.0",
    "@mike-north/js-lib-renovate-config": "0.0.1",
    "@mike-north/js-lib-semantic-release-config": "0.0.0-development",
    "broccoli-asset-rev": "3.0.0",
    "coveralls": "3.0.2",
    "ember-cli": "3.5.0",
    "ember-cli-app-version": "3.2.0",
    "ember-cli-autoprefixer": "0.8.1",
    "ember-cli-code-coverage": "0.4.2",
    "ember-cli-dependency-checker": "3.0.0",
    "ember-cli-eslint": "4.2.3",
    "ember-cli-github-pages": "0.2.0",
    "ember-cli-htmlbars-inline-precompile": "1.0.2",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-shims": "1.2.0",
    "ember-cli-template-lint": "1.0.0-beta.2",
    "ember-cli-uglify": "2.1.0",
    "ember-code-snippet": "2.3.1",
    "ember-composability": "0.3.8",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-resolver": "5.0.1",
    "ember-source": "3.5.1",
    "ember-source-channel-url": "1.1.0",
    "ember-truth-helpers": "2.1.0",
    "ember-try": "1.0.0-beta.3",
    "eslint-plugin-ember": "5.4.0",
    "eslint-plugin-node": "8.0.0",
    "greenkeeper-lockfile": "1.12.0",
    "husky": "1.1.2",
    "loader.js": "4.7.0",
    "sass": "1.14.3",
    "travis-deploy-once": "5.0.9",
    "semantic-release": "15.10.8"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.1",
    "chalk": "2.3.0",
    "ember-cli-babel": "^7.1.3",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-sass": "^8.0.1",
    "material-design-lite": "~1.0.6",
    "node-sass": "^4.9.4",
    "rsvp": "^4.8.4"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://mike.works/ember-material-lite"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
